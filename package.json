{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build && npm run clean:cache",
    "build:cloudflare": "prisma generate && npx @cloudflare/next-on-pages && npm run clean:cache",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "clean:cache": "rimraf .next/cache",
    "optimize:cloudflare": "node scripts/optimize-for-cloudflare.js",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.22.0",
    "@prisma/client": "^6.10.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "lucide-react": "^0.515.0",
    "next": "15.2.3",
    "postcss": "^8",
    "prisma": "^6.10.1",
    "react": "^18",
    "react-dom": "^18",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "eslint": "^8",
    "eslint-config-next": "15.2.3",
    "glob": "^11.0.3"
  }
}
